<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Users</title>
    <style>
      span {
        color: brown;
      }
      .btn {
        text-decoration: none;
        color: white;
        background-color: green;
        padding: 7px 12px;
        border-radius: 4px;
      }
    </style>
  </head>
  <body>
    <h1>Users List</h1>
    <% queryResult.forEach(user=> { %>

    <h2><%= user.fullname%> <span> Age: <%=user.age%> </span></h2>

    <% }) %>

    <a href="/newuser" class="btn"> + Add New User </a>
  </body>
</html>

